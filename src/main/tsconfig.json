{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "lib": ["es2015", "dom"],
        "allowJs": true,
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
      "node_modules",
      "src/renderer"
    ]
}
